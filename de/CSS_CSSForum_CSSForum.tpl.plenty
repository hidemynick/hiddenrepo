/*
  root element for the scrollable.
  when scrolling occurs this element stays still.
*/

.pmscrollable {
    /* required settings */
    position:relative;
    overflow:hidden;
    width: 785px;
    height:160px;
	padding-top:10px;
}

.tools-slide {
	
}

/*
   root element for scrollable items. Must be absolutely positioned
   and it should have a extremely large width to accomodate scrollable
   items.  it's enough that you set the width and height for the root
   element and not for this element.
*/

.pmscrollable .items {
    /* this cannot be too large */
    width:20000em;
    position:absolute;
    clear:both;
}

.pmscrollable .items > div {
    float:left;
    width:800px;
}

.pmscrollable .items > div > a {
display: block;
width: 184px;
height: 134px;
margin: 0 5px;
float: left;
background: #f7f7f7;
border: solid 1px #ffffff;
text-align: center;
	-moz-border-radiu: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	-moz-box-shadow: 0px 0px 6px #cccccc;
	-webkit-box-shadow: 0px 0px 6px #cccccc;
	box-shadow: 0px 0px 6px #cccccc;
}

/* single scrollable item */
.pmscrollable .items > div > a > img {
    margin: 14px 0 0;
}
/* single scrollable text */
.pmscrollable .items > div > a > span {
    display: block;
    margin: 7px 0 0;
    color: #666666;
    font-weight: 400;
}
.pmscrollable .items > div > a:link:hover,
.pmscrollable .items > div > a:visited:hover {
    text-decoration: none;
}

/* individual style */
.pmscrollable .items > div > a.pmWebselling > img { padding-top: 12px; }
.pmscrollable .items > div > a.pmTopProduktHandel > img { position: relative; top: -1px; }
.pmscrollable .items > div > a.pmTrustedshops > img { padding-top: 10px; }
.pmscrollable .items > div > a.pmEhi > img { padding-top: 6px; }
.pmscrollable .items > div > a.pmPaypal > img { padding-top: 14px; }


/* active item */
.pmscrollable .active {
    border:2px solid #000;
    position:relative;
    cursor:default;
}

/* this makes it possible to add next button beside scrollable */
.pmscrollable {
    float:left;
	margin-left:26px;
	margin-top:0px;
}

/* prev, next, prevPage and nextPage buttons */
a.browse {
    /*background:url(/layout/pm/images/layout/hori_large.png) no-repeat;*/
    display:block;
    width:30px;
    height:30px;
    float:left;
    margin:40px 10px;
	
	margin-top:60px;
	
    cursor:pointer;
    font-size:1px;

-moz-border-radius: 15px;
-webkit-border-radius: 15px;
border-radius: 15px;
background: #eeeeee;
text-align: center;
line-height: 30px;
-moz-box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.30), 0 0 0 1px rgba(255, 255, 255, 0.5) inset;
-webkit-box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.30), 0 0 0 1px rgba(255, 255, 255, 0.5) inset;
box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.30), 0 0 0 1px rgba(255, 255, 255, 0.5) inset;
-webkit-transition: background 150ms ease;
-moz-transition: background 150ms ease;
-ms-transition: background 150ms ease;
-o-transition: background 150ms ease;
transition: background 150ms ease;
}
a.browse > span {
border-style: solid;
border-width: 6px 12px;
border-color: transparent #ffffff;
}
a.browse.prev > span {
border-left: none;
margin-right: 3px;
}
a.browse.next > span {
border-right: none;
margin-left: 3px;
}

/* right */
a.right { background-position: 0 -30px; clear:right; margin-right: 20px; float:right;}
a.right:hover { /*background-position:-30px -30px;*/ }
a.right:active { /*background-position:-60px -30px;*/ }
/* left */
a.left { margin-left: 20px; }
a.left:hover  { /*background-position:-30px 0;*/ }
a.left:active { /*background-position:-60px 0;*/ }

/* hover, focus */
a.browse:hover,
a.browse:focus { 
background: #e8e8e8; 
-moz-box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.4), 0 0 0 1px rgba(255, 255, 255, 0.3) inset;
-webkit-box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.4), 0 0 0 1px rgba(255, 255, 255, 0.3) inset;
box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.4), 0 0 0 1px rgba(255, 255, 255, 0.3) inset;
}
/* active */
a.browse:active { 
background: #e4e4e4; 
-moz-box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.15), 0 2px 3px 0 rgba(0, 0, 0, 0.3) inset;
-webkit-box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.15), 0 2px 3px 0 rgba(0, 0, 0, 0.3) inset;
box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.15), 0 2px 3px 0 rgba(0, 0, 0, 0.3) inset;
}
a.browse:active > span {
position: relative;
top: 1px;
}

/* up and down */
a.up, a.down  {
    background:url(/media/img/scrollable/arrow/vert_large.png) no-repeat;
    float: none;
    margin: 10px 50px;
}

/* up */
a.up:hover { background-position:-30px 0; }
a.up:active { background-position:-60px 0; }

/* down */
a.down { background-position: 0 -30px; }
a.down:hover { background-position:-30px -30px; }
a.down:active { background-position:-60px -30px; }


/* disabled navigational button */
a.disabled {
    visibility:hidden !important;
