/* NEW LANGUAGE DROPDOWN */

/* ul reset */
ul.langDropdown,
.langDropdown ul {
    list-style: none;
	padding: 0;
    margin: 0;
}
ul.langDropdown > li,
.langDropdown ul > li {
	position: relative;
}

/* dropdown container */
.langDropdown { 
	line-height: 18px;
}
/* selection */
.langDropdownSelection {
	display: none;
	position: absolute;
}
/* width all */
.langDropdown,
.langDropdownSelection {
	width: 100px; 
}

/* a */
.langDropdown li > a { 
	display: block; 
	height: 16px;
	padding: 5px 30px 5px 7px;
	color: #ccc;
	text-decoration: none;
	background-color: #333;
	
	-webkit-transition: background-color .3s ease, color .3s ease; 
	-moz-transition: background-color .3s ease, color .3s ease; 
	-o-transition: background-color .3s ease, color .3s ease; 
	-ms-transition: background-color .3s ease, color .3s ease; 
	transition: background-color .3s ease, color .3s ease; 
}
/* a hover/focus */
.langDropdown li > a:hover,
.langDropdown li > a:focus { 
	color: #fff;
	background-color: #404040;
}
/* a selected */
.langDropdown > li > a,
.langDropdown > li > a:hover,
.langDropdown > li > a:focus { 
	padding-top: 1px;
	padding-bottom: 1px;
	background-color: #333;
	line-height: 16px;
	color: #fff;
	cursor: default;
}

/* a */
.langDropdown li > a { 
	background-image: url(/layout/pm/images/icon/flag_sprite_vertical.png);
	background-repeat: no-repeat;
}
/* a selected lang */
.langDropdown > li > a.de { background-position: 77px 3px; }
.langDropdown > li > a.en { background-position: 77px -22px; }
.langDropdown > li > a.fr { background-position: 77px -47px; }
.langDropdown > li > a.it { background-position: 77px -72px; }

/* a lang */
.langDropdown li.de > a { background-position: 77px 8px; }
.langDropdown li.en > a { background-position: 77px -17px; }
.langDropdown li.fr > a { background-position: 77px -42px; }
.langDropdown li.it > a { background-position: 77px -67px; }

/* show sublevel */
.langDropdown > li:hover > ul,
.langDropdown > li > a:focus + ul {
	display: block;
}

/* border-radius */

/* a selected */
.langDropdown > li > a { 
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
}
/* a selected hover */
.langDropdown > li:hover > a,
.langDropdown > li > a:focus { 
	-moz-border-radius: 5px 5px 0 0;
	-webkit-border-radius: 5px 5px 0 0;
	border-radius: 5px 5px 0 0;
}
/* selection a last */
.langDropdown > li > ul > li:last-child > a { 
	-moz-border-radius: 0 0 5px 5px;
	-webkit-border-radius: 0 0 5px 5px;
	border-radius: 0 0 5px 5px;
}

/* ################################################################################
end of language dropdown
