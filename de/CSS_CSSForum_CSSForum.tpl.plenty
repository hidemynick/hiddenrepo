/* ################################################################################
main navigation with dropdown
################################################################################ */

/* navi container */
.pmmainnavigation {
height: 38px;
border: none;
-moz-border-radius: 4px;
-webkit-border-radius: 4px;
border-radius: 4px;
}
/* navi container & a level 1 */
.pmmainnavigation,
.pmmainnavigation > ul > li > a {
background: #008ebd url("//www.plentymarkets.eu/layout/pm/images/layout/navi-verlauf.png") repeat-x;
}

/* li level1 */
.pmmainnavigation > ul > li {
float: left;
}
/* ul, li level >=2 */
.pmmainnavigation > ul li, 
.pmmainnavigation > ul ul {
position: relative;
background: none;
}

/* a all */
.pmmainnavigation ul > li > a {
display: block;
zoom: 1;
padding: 10px 15px;
white-space: nowrap;
font-weight: bold;
background: none;

-webkit-transition: background-color .3s ease, color .3s ease; 
-moz-transition: background-color .3s ease, color .3s ease; 
-o-transition: background-color .3s ease, color .3s ease; 
-ms-transition: background-color .3s ease, color .3s ease; 
transition: background-color .3s ease, color .3s ease; 
}
/* a level1 */
.pmmainnavigation > ul > li > a {
color: #fff;
border-right: 1px solid #0580a8;
border-left: 1px solid #45a6c6;
}

/* a first (home) */
.pmmainnavigation > ul > li:first-child a {

-moz-border-radius: 4px 0 0 4px;
-webkit-border-radius: 4px 0 0 4px;
border-radius: 4px 0 0 4px;

border: none;
border-right: 1px solid #0580a8;
}

/* a hover all */
.pmmainnavigation ul li a:hover,
.pmmainnavigation ul li a:focus {
text-decoration: none;
}

/* a (li) hover level1, active level1 */
.pmmainnavigation > ul > li > a:hover, 
.pmmainnavigation > ul > li > a:focus,  
.pmmainnavigation > ul > li.active > a, 
.pmmainnavigation > ul > li:hover > a,
.pmmainnavigation > ul > li.focus > a { /* UPDATE */
background-color: #fff;
color: #008ebd;
}

/* ul level >=2 */
.pmmainnavigation > ul > li ul {
display: none;
/*left: -99000px;*/
position: absolute;
z-index: 2000;
	
background: #fff; /* Old browsers */
background: -moz-linear-gradient(top, rgba(255,255,255,1) 0%, rgba(246,246,246,1) 47%, rgba(237,237,237,1) 100%);
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(47%,rgba(246,246,246,1)), color-stop(100%,rgba(237,237,237,1)));
background: -webkit-linear-gradient(top, rgba(255,255,255,1) 0%,rgba(246,246,246,1) 47%,rgba(237,237,237,1) 100%);
background: -o-linear-gradient(top, rgba(255,255,255,1) 0%,rgba(246,246,246,1) 47%,rgba(237,237,237,1) 100%);
background: -ms-linear-gradient(top, rgba(255,255,255,1) 0%,rgba(246,246,246,1) 47%,rgba(237,237,237,1) 100%); 
background: linear-gradient(to bottom, rgba(255,255,255,1) 0%,rgba(246,246,246,1) 47%,rgba(237,237,237,1) 100%);
/*filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#ededed',GradientType=0 );*/ /* ie doesn't seem to like combination with border radius */

-moz-box-shadow: 0 2px 5px 3px rgba(0, 0, 0, 0.3);
-webkit-box-shadow: 0 2px 5px 3px rgba(0, 0, 0, 0.3);
box-shadow: 0 2px 5px 3px rgba(0, 0, 0, 0.3);

-moz-border-radius: 5px;
-webkit-border-radius: 5px;
border-radius: 5px;
}

/* li level >=2 */
.pmmainnavigation > ul > li ul > li {
border-bottom: 1px solid #ccc;
background: none;
}

/* a level >=2 */
.pmmainnavigation > ul > li ul > li > a {
color: #5e5e5e;
padding: 5px 15px;
}

/* a hover / li current level >=2 */ /* content-navigation uses .page_active & .dir_active | category-navigation uses .active */
.pmmainnavigation > ul > li ul > li > a:hover,
.pmmainnavigation > ul > li ul > li > a:focus,
.pmmainnavigation > ul > li ul > li.page_active > a,
.pmmainnavigation > ul > li ul > li.dir_active > a,
.pmmainnavigation > ul > li ul > li.active > a {
background-color: #008ebd;
color: #fff;
}

/* ul level >=3 */
.pmmainnavigation > ul > li ul > li > ul {
position: absolute;
left: 100%;
top: 0;
}

/* show sublevel */

/* level1 */
.pmmainnavigation > ul > li:hover > ul, 
/*.pmmainnavigation > ul > li:active > ul, 
.pmmainnavigation > ul:focus, 
.pmmainnavigation > ul:active, */
.pmmainnavigation ul > li > a:focus + ul,

.pmmainnavigation ul > li > ul.focus { /* UPDATE */
display: block;
/*left: 0;*/
}
/* level >=2 */
.pmmainnavigation > ul > li ul > li:hover > ul, 
/*.pmmainnavigation > ul > li ul > li:active > ul, 
.pmmainnavigation > ul > li ul:focus, 
.pmmainnavigation > ul > li ul:active, */
.pmmainnavigation > ul > li ul > li > a:focus + ul,

.pmmainnavigation > ul > li ul > li > ul.focus { /* UPDATE */
display: block;
/*left: 100%;*/
}


/* BORDER BOTTOM */

/* li last level >=2 */
.pmmainnavigation > ul > li ul > li:last-child,
.pmmainnavigation > ul > li ul > li.categoryId394 {
border:none;
} /*adapted to hidden last child container*/


/* BORDER RADIUS */

/* a first level >=2 */
.pmmainnavigation > ul > li ul > li:first-child > a {

-moz-border-radius-topleft: 4px;
-moz-border-radius-topright: 4px;
-webkit-border-top-left-radius: 4px;
-webkit-border-top-right-radius: 4px;
border-top-left-radius: 4px;
border-top-right-radius: 4px;
}

/* a last level >=2 */
.pmmainnavigation > ul > li ul > li:last-child > a,
.pmmainnavigation > ul > li ul > li.categoryId394 > a {	

-moz-border-radius-bottomleft: 4px;
-moz-border-radius-bottomright: 4px;
-webkit-border-bottom-left-radius: 4px;
-webkit-border-bottom-right-radius: 4px;
border-bottom-left-radius: 4px;
border-bottom-right-radius: 4px;
} /*adapted to hidden last child container*/


/* ARROW */

/* a dir (arrow) */
.pmmainnavigation > ul > li  ul > li.dir > a {
padding-right: 25px;
background-image: url("//www.plentymarkets.eu/layout/pm/images/icon/navi-arrow-sprite.png");
background-repeat: no-repeat;
background-position: top right;
}
/* a dir hover / current (arrow) */
.pmmainnavigation > ul > li  ul > li.dir > a:hover,
.pmmainnavigation > ul > li  ul > li.dir > a:focus, /* UPDATE */
.pmmainnavigation > ul > li  ul > li.dir_active > a,
.pmmainnavigation > ul > li  ul > li.page_active > a {
background-position: bottom right;
}


/* SPECIAL */

/* special style level1 */
.pmNavigationBorderHelper {
border: none:
padding: 0;
margin: 0;
display: block;
float: left;
height: 38px;
width: 0;
border-left: 1px solid #45A6C6;
}


