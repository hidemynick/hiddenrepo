/* root element for tabs  */
ul.css-tabs,
.pmHasTabs > ul {
    height: 30px;
    margin: 0 !important;
    padding: 0;
    position: relative;
    top: 1px;
    z-index: 1000;
}

/* single tab */
ul.css-tabs li,
.pmHasTabs > ul > li {
    float: left;
    list-style-type: none;
    margin: 0 -5px;
    overflow: hidden;
    padding: 5px 5px 1px;
    position: relative;
    top: -5px;
}

/* link inside the tab. uses a background image */
ul.css-tabs a,
.pmHasTabs > ul > li > a {

border: 2px solid #FFFFFF !important;
border-bottom: none !important;

    background-color: #EFEFEF;
    border-bottom: 0 none;
    -moz-border-radius: 3px 3px 0 0;
    -webkit-border-radius: 3px 3px 0 0;
    border-radius: 3px 3px 0 0;
    -moz-box-shadow: 0 0 4px #CCCCCC, 0 -9px 6px -12px #CCCCCC inset;
    -webkit-box-shadow: 0 0 4px #CCCCCC, 0 -9px 6px -12px #CCCCCC inset;
    box-shadow: 0 0 4px #CCCCCC, 0 -9px 6px -12px #CCCCCC inset;
    color: #333333;
    display: block;
    float: left;
    font-weight: bold;
    height: 16px;
    margin-right: 2px;
    outline: 0 none;
    padding: 5px 12px;
    position: relative;
    text-decoration: none;
    top: 1px;

font-size: 90%;
}

ul.css-tabs a:hover,
.pmHasTabs > ul > li > a:hover,
.pmHasTabs > ul > li > a:focus,
.pmHasTabs > ul > li > a:active {
    background-color:#F7F7F7;
    color:#008ebd;
}

/* selected tab */
ul.css-tabs a.current,
.pmHasTabs > ul > li.current > a {
    background-color: white;
    border-bottom: 1px solid white;
    -moz-box-shadow: 0 0 6px #CCCCCC;
    -webkit-box-shadow: 0 0 6px #CCCCCC;
    box-shadow: 0 0 6px #CCCCCC;
    color: #333333;
    cursor: default;
    overflow: hidden;
    position: relative;
    z-index: 1001;
}

