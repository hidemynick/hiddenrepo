/* CONTRACT BUILDER */

.contractBuilder {
	font-size: 12px;
}

input::-moz-focus-inner { border: 0; padding: 0; }

.contractBuilderItemDate,
.contractBuilderItemPrice {
	border-width: 1px;
	border-color: #e0e0e0;
	font-size: 12px;
}


ul.contractBuilderList > li.center {
	text-align: center;
}

.contractBuilderItem {
	display: inline-block;
	width: 330px;
	padding: 5px 10px;
	text-align: center;
	vertical-align: top;
}
.contractBuilderItem + .contractBuilderItem {
	margin-left: 10px;
}

a.contractBuilderItem,
a.contractBuilderItem:hover,
a.contractBuilderItem:focus {
	color: inherit;
	text-decoration: inherit;
}

.contractBuilderItemDate,
.contractBuilderItemTitle,
.contractBuilderItemAdditional,
.contractBuilderItemDetailsListWrapper,
.contractBuilderItemAdditionalHint,
.contractBuilderItemChange {
	display: block;
}

.contractBuilderItemDate {
	border-style: none none solid;
	padding: 5px 0;
	text-align: left;
	color: #008EBD;
	font-size: 15px;
	font-weight: bold;
}
.contractBuilderItemDateHint {
	font-size: 13px;
	font-weight: normal;
}

.contractBuilderItemTitle {
	padding-top: 10px;
	line-height: 1.5;
	color: #444;
}
.contractBuilderItemTitle,
.contractBuilderItemTitle.isNumber.placeholder {
	font-size: 26px;
}
.contractBuilderItemTitle.isNumber.placeholder {
	line-height: 2.4em;
}
.contractBuilderItemTitle.isNumber {
	font-size: 40px;
	min-height: 60px;
}

.contractBuilderItemTitle.isNumber + .contractBuilderItemAdditional {
	margin-top: -7px;
}
.contractBuilderTwoColumns .contractBuilderItemAdditional {
	text-align: left;
	padding-top: 10px;
}
.contractBuilderTwoColumns .contractBuilderItemTitle {
	padding: 0;
}
.contractBuilderItem > .contractBuilderItemAdditional {
	margin-top: 10px;
}
.contractBuilderItemAdditional + .contractBuilderItemTitle {
	padding: 0;
}

.contractBuilderItemAdditionalHint {
	font-size: 11px;
	color: #666;
}
.contractBuilderItemAdditionalHint:before { content: "(" }
.contractBuilderItemAdditionalHint:after { content: ")" }

.contractBuilderItemAdditional + .contractBuilderItemAdditionalHint {
	margin-top: 5px;
}
.contractBuilderItemAdditional > .contractBuilderItemAdditionalHint {
	display: inline;
	color: #008EBD;
}

