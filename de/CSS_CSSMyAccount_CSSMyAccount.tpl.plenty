/* slider */

.ui-slider {
	position: relative;
	display: block;
	margin: 19px 15px 18px;
	height: 6px;
	border: 1px solid #888;
	
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	
	-moz-box-shadow: 0 2px 3px 0 rgba(0, 0, 0, 0.3) inset;
	-webkit-box-shadow: 0 2px 3px 0 rgba(0, 0, 0, 0.3) inset;
	box-shadow: 0 2px 3px 0 rgba(0, 0, 0, 0.3) inset;
	
}
.ui-slider-handle {
	position: absolute;
	width: 16px;
	height: 16px;
	margin: -6px 0 0 -6px;
	border: 1px solid #888;
	background-color: #e0e0e0;
		
	-moz-border-radius: 50%;
	-webkit-border-radius: 50%;
	border-radius: 50%;
	
	-moz-box-shadow: 0 3px 4px 0 rgba(255, 255, 255, 0.8) inset, 0 1px 3px 0 rgba(0, 0, 0, 0.3);
	-webkit-box-shadow: 0 3px 4px 0 rgba(255, 255, 255, 0.8) inset, 0 1px 3px 0 rgba(0, 0, 0, 0.3);
	box-shadow: 0 3px 4px 0 rgba(255, 255, 255, 0.8) inset, 0 1px 3px 0 rgba(0, 0, 0, 0.3);
}
.ui-slider-handle.ui-state-hover,
.ui-slider-handle.ui-state-focus {
	border-color: #666;
	background-color: #f0f0f0;
	
	-moz-box-shadow: 0 3px 4px 0 rgba(255, 255, 255, 1) inset, 0 1px 3px 0 rgba(0, 0, 0, 0.4);
	-webkit-box-shadow: 0 3px 4px 0 rgba(255, 255, 255, 1) inset, 0 1px 3px 0 rgba(0, 0, 0, 0.4);
	box-shadow: 0 3px 4px 0 rgba(255, 255, 255, 1) inset, 0 1px 3px 0 rgba(0, 0, 0, 0.4);
}
.ui-slider-handle.ui-state-focus {
	border-color: #333;
	background-color: #fff;
}

