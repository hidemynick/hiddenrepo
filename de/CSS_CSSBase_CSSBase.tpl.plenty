/* speech bubbles, added 2015-01-19 by Felix Dausch */
.speech-bubble {
    position: relative;
    border-radius: 5px;
    border: #ccc solid 1px;
    padding: 15px;
}

.equal .speech-bubble {  
    height: 100%;
}

.speech-bubble.indented {
    margin-left: 0px;
}

.speech-bubble:before,
.speech-bubble:after {
    content: "";
    position: absolute;
    border-width: 15px;
    display: block;
}

.speech-bubble.top:before,
.speech-bubble.bottom:before,
.speech-bubble.top-left:before,
.speech-bubble.bottom-left:before,
.speech-bubble.top-right:before,
.speech-bubble.bottom-right:before {   
    border-color: #ccc transparent;
}

.speech-bubble.left:before,
.speech-bubble.right:before,
.speech-bubble.left-top:before,
.speech-bubble.right-top:before,
.speech-bubble.left-bottom:before,
.speech-bubble.right-bottom:before {
    border-color: transparent #ccc;
}

.speech-bubble.top:after,
.speech-bubble.bottom:after,
.speech-bubble.top-left:after,
.speech-bubble.bottom-left:after,
.speech-bubble.top-right:after,
.speech-bubble.bottom-right:after {
    border-color: #fff transparent;
}

.speech-bubble.left:after,
.speech-bubble.right:after,
.speech-bubble.left-top:after,
.speech-bubble.right-top:after,
.speech-bubble.left-bottom:after,
.speech-bubble.right-bottom:after {
    border-color: transparent #fff;
}

/* speech bubble top */
.speech-bubble.top.indented,
.speech-bubble.top-left.indented,
.speech-bubble.top-right.indented {
    margin-top: 15px;
}

.speech-bubble.top:before,
.speech-bubble.top:after,
.speech-bubble.top-left:before,
.speech-bubble.top-left:after,
.speech-bubble.top-right:before,
.speech-bubble.top-right:after{
    border-style: none solid solid;   
    top: -15px;
}

.speech-bubble.top:before {
    left: 50%;
    margin-left: -15px;
}

.speech-bubble.top:after {
    padding-top: 1px;
    margin-left: -15px;
    left: 50%;
}

.speech-bubble.top-left:before {
    left: 40px;
}

.speech-bubble.top-left:after {
    padding-top: 1px;
    left: 40px;
}

.speech-bubble.top-right:before {
    right: 40px;
}

.speech-bubble.top-right:after {
    padding-top: 1px;
    right: 40px;
}

/* speech bubble bottom */
.speech-bubble.bottom.indented,
.speech-bubble.bottom-left.indented,
.speech-bubble.bottom-right.indented {
    margin-bottom: 15px;
}

.speech-bubble.bottom:before,
.speech-bubble.bottom:after,
.speech-bubble.bottom-left:before,
.speech-bubble.bottom-left:after,
.speech-bubble.bottom-right:before,
.speech-bubble.bottom-right:after{
    border-style: solid solid none;
    bottom: -15px;
}

.speech-bubble.bottom:before {
    left: 50%;
    margin-left: -15px;
}

.speech-bubble.bottom:after {
    padding-bottom: 1px;
    left: 50%;
    margin-left: -15px;
}

.speech-bubble.bottom-left:before {
    left: 40px;
}

.speech-bubble.bottom-left:after {
    padding-bottom: 1px;
    left: 40px;
}

.speech-bubble.bottom-right:before {
    right: 40px;
}

.speech-bubble.bottom-right:after {
    padding-bottom: 1px;
    right: 40px;
}

/* speech bubble left */
.speech-bubble.left.indented,
.speech-bubble.left-top.indented,
.speech-bubble.left-bottom.indented {
    margin-left: 15px;
}

.speech-bubble.left:before,
.speech-bubble.left:after,
.speech-bubble.left-top:before,
.speech-bubble.left-top:after,
.speech-bubble.left-bottom:before,
.speech-bubble.left-bottom:after{
    border-style: solid solid solid none;
    left: -15px;
}

.speech-bubble.left:before {
    top: 50%;
    margin-top: -15px;
}

.speech-bubble.left:after {
    padding-left: 1px;
    top: 50%;
    margin-top: -15px;
}

.speech-bubble.left-top:before {
    top: 40px;
}

.speech-bubble.left-top:after {
    padding-left: 1px;
